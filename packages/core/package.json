{
  "name": "@paretojs/core",
  "version": "2.0.0-beta.1",
  "homepage": "https://paretojs.tech/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/childrentime/pareto.git",
    "directory": "packages/core"
  },
  "keywords": [
    "react",
    "meta",
    "ssr",
    "server side rendering",
    "streamed ssr",
    "streamed server side rendering",
    "node.js",
    "express",
    "react server"
  ],
  "description": "lightweight streamed ssr framework",
  "bugs": {
    "url": "https://github.com/childrentime/pareto/issues"
  },
  "files": [
    "dist",
    "env.d.ts",
    "config.ts",
    "dist-bin",
    "bin",
    "useStyles"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      }
    },
    "./env": {
      "types": "./env.d.ts"
    },
    "./config": {
      "types": "./config.ts"
    },
    "./node": {
      "require": {
        "types": "./dist/node.d.ts",
        "default": "./dist/node.js"
      }
    },
    "./client": {
      "require": {
        "types": "./dist/client.d.ts",
        "default": "./dist/client.js"
      }
    }
  },
  "types": "./dist/main.d.ts",
  "bin": {
    "pareto": "bin/index.js"
  },
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production node rspack.config.js && tsc",
    "build:bin": "tsup",
    "dev:bin": "tsup --watch",
    "dev": "rm -rf dist && node rspack.config.js && tsc -w",
    "release:prepare": "bump",
    "publish:ci": "tsx scripts/publish.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/serialize-javascript": "^5.0.4",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/webpack-hot-middleware": "^2.25.9",
    "@types/webpack-node-externals": "^3.0.4"
  },
  "peerDependencies": {
    "express": "^4.0.0",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-helmet-async": "^2.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-transform-class-static-block": "^7.26.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@paretojs/monitor": "workspace:*",
    "@rspack/cli": "^1.2.7",
    "@rspack/core": "^1.2.7",
    "@rspack/plugin-react-refresh": "^1.0.1",
    "@types/assets-webpack-plugin": "^7.1.6",
    "assets-webpack-plugin": "^7.1.1",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "cac": "^6.7.14",
    "clear-module": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "fs-extra": "^11.3.0",
    "jiti": "^2.4.2",
    "loader-utils": "^3.3.1",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "react-refresh": "^0.16.0",
    "sass": "^1.85.1",
    "sass-loader": "^16.0.5",
    "serialize-javascript": "^6.0.2",
    "webpack": "^5.98.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-node-externals": "^3.0.0"
  }
}
