{
  "name": "create-pareto",
  "version": "0.0.10-beta.1",
  "type": "module",
  "description": "pareto template generator",
  "author": "childrentime",
  "contributors": [
    "Lesenelir"
  ],
  "bin": {
    "create-pareto": "./index.js"
  },
  "files": [
